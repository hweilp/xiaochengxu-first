.list{
  width: 100%;
  box-sizing: border-box;
  padding:0 20px; 
}
.list-item-text{
  width: 100%;
}
.list-content{
  background: #d2d2d2;
  width: 100%;
  padding: 20rpx 0;
  box-sizing: border-box;
}
.contents{
 padding: 0 20rpx;
}
.contents .title-text{
  font-size: 30rpx;
  color: #007AFF;
  margin-bottom: 15rpx;
}
.attr-content{
  width: 97%;
  background: white;
  padding: 10rpx;
}
.attr-list{
  width: 100%;
  display: flex;
  justify-content: space-between;
  box-sizing: border-box;
  flex-direction: row;
  text-align: center;
  border-bottom: 1px solid #d2d2d2;
  padding-top: 8rpx;
  padding-bottom: 8rpx;
}
.attr-list:last-child{
  padding-bottom: 0;
  border-bottom: none;
}
.attr-list text{
  flex: 1;
  font-size: 20rpx;
}